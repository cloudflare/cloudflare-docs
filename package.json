{
  "private": true,
  "version": "1.0.0",
  "name": "cloudflare-docs",
  "repository": "cloudflare/cloudflare-docs",
  "description": "Cloudflare Developer Docs",
  "scripts": {
    "dev": "npx hugo serve -D",
    "lint": "npx tsm bin/format.ts --check",
    "crawl": "npx tsm bin/crawl.ts",
    "prebuild": "npx tsm bin/highlight.ts replace",
    "build": "npx hugo --minify",
    "postbuild": "npx tsm bin/highlight.ts restore"
  },
  "dependencies": {
    "fromnow": "3.0.1"
  },
  "devDependencies": {
    "@types/node": "16.11.12",
    "@types/prettier": "2.4.3",
    "@types/prismjs": "1.16.6",
    "hugo-bin": "^0.89.0",
    "node-html-parser": "5.2.0",
    "prettier": "2.5.1",
    "prismjs": "^1.29.0",
    "tsm": "2.2.2"
  }
}
